<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
  PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
  "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
	<context id="table" targetRuntime="MyBatis3">
		<commentGenerator>
			<property name="suppressAllComments" value="false" />
		</commentGenerator>
		<jdbcConnection driverClass="com.mysql.jdbc.Driver" connectionURL="jdbc:mysql://localhost:3306/questionnaire_survey" userId="root" password="root" />
		<javaModelGenerator targetPackage="com.vo" targetProject=".\src">
			<property name="enableSubPackages" value="false" />
			<property name="trimStrings" value="true" />
		</javaModelGenerator>
		<sqlMapGenerator targetPackage="com.dao" targetProject=".\src">
			<property name="enableSubPackages" value="false" />
		</sqlMapGenerator>
		<javaClientGenerator type="XMLMAPPER" targetPackage="com.dao" targetProject=".\src">
			<property name="enableSubPackages" value="false" />
		</javaClientGenerator>
		<table tableName="admin"></table>
		<table tableName="user"></table>
		<table tableName="sort"></table>
		<table tableName="dict_templet"></table>
		<table tableName="design_templet"></table>
		<table tableName="dict_templet_index"></table>
		<table tableName="design_templet_index"></table>
		<table tableName="design_single"></table>
		<table tableName="design_single_option"></table>
		<table tableName="design_checkbox"></table>
		<table tableName="design_checkbox_option"></table>
		<table tableName="design_text"></table>
		<table tableName="dict_single"></table>
		<table tableName="dict_single_option"></table>
		<table tableName="dict_checkbox"></table>
		<table tableName="dict_checkbox_option"></table>
		<table tableName="dict_text"></table>
		<table tableName="single_answer_count"></table>
		<table tableName="checkbox_answer_count"></table>
		<table tableName="text_answer"></table>
		<table tableName="user_login_log"></table>
		<table tableName="user_login_result"></table>
		<table tableName="user_operate_log"></table>
		<table tableName="user_operate_project"></table>
	</context>
</generatorConfiguration>